                    <MaxLength>10</MaxLength>
                    <CallInfo>
                      <CallName>getCompatibilitySearchNames</CallName>
                      <RequiredInput>Yes</RequiredInput>
                    </CallInfo>
                  </xs:appinfo>
                </xs:annotation>
              </xs:element>
